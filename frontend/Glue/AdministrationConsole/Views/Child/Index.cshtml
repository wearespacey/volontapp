@model IEnumerable<Child>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nom</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var i in Model)
        {
            <tr>
                <td>@i.Id</td>
                <td>@Html.ActionLink(@i.FirstName +" "+ i.LastName, "Detail", "Child", new { id = i.Id })</td>
            </tr>
        }
    </tbody>
</table>